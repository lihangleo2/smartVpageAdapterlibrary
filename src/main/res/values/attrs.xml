<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleIndicator">
        <!-- 圆形指示器半径 -->
        <attr name="lh_indicator_radius" format="dimension" />
        <!-- 指示器选中颜色 -->
        <attr name="lh_indicator_selectColor" format="color" />
        <!-- 指示器未选中颜色 -->
        <attr name="lh_indicator_unselectColor" format="color" />
        <!-- 指示器间隔距离 -->
        <attr name="lh_indicator_space" format="dimension" />
        <!-- 指示器是否跟随viewPager2滑动 -->
        <attr name="lh_indicator_scrollWithViewPager2" format="boolean" />
        <!-- 不同模式 -->
        <attr name="lh_indicator_mode">
            <!-- 充满模式 -->
            <enum name="fill" value="1" />
            <!-- 边框模式 -->
            <enum name="stroke" value="2" />
        </attr>
        <!-- stroke下模式下边框width宽度 -->
        <attr name="lh_indicator_strokeWidth" format="dimension" />
    </declare-styleable>


    <declare-styleable name="LineIndicator">
        <!-- 指示器选中颜色 -->
        <attr name="line_indicator_selectColor" format="color" />
        <!-- 指示器未选中颜色 -->
        <attr name="line_indicator_unselectColor" format="color" />
        <!-- 指示器间隔距离 -->
        <attr name="line_indicator_space" format="dimension" />
        <!-- 指示器是否跟随viewPager2滑动 -->
        <attr name="line_indicator_scrollWithViewPager2" format="boolean" />
    </declare-styleable>


